<configuration>
    <appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
        <encoder>
            <pattern>%d{HH:mm:ss.SSS} [%thread{10}] %-5level %logger{36}%n%msg%n%n</pattern>
        </encoder>
    </appender>
    <logger name="akka.io" level="INFO"/>
    <root level="debug">
        <appender-ref ref="stdout"/>
    </root>
</configuration>